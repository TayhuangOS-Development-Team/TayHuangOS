OBJECTSDIR := $(OBJECTSDIR)/{SUBMODULE}/
BINDIR := $(BINDIR)/{SUBMODULE}/
{SUBMODULE_UPPER}DIR := $(ROOTDIR)/{SUBMODULE}/
export OBJECTSDIR {SUBMODULE}DIR

#编译
.PHONY: build
build:
	$(CD) {SUBMODULE0} && $(MAKE) build

#清理
.PHONY: clean
clean:
	$(CD) {SUBMODULE0} && $(MAKE) clean

.PHONY: image
image:
	$(CD) {SUBMODULE0} && $(MAKE) image